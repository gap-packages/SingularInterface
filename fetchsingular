#!/bin/sh -ev

mkdir -p Singular

git clone -b master https://github.com/Singular/Sources.git Singular

cd Singular

# Create some directories such that one can manually add links to GAP's
# gmp.h and libgmp.a and libgmp.la:
mkdir -p `./singuname.sh`
mkdir -p `./singuname.sh`/include
mkdir -p `./singuname.sh`/lib
